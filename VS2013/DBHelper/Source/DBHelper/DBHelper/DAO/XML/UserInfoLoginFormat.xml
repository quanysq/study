SELECT a.UserID,a.Usercode,a.UserName,a.Pwd,a.IsAddSelf,a.IsEditSelf,a.IsDeleteSelf,a.IsEditOther,a.IsDeleteOther  
FROM UserInfo AS a 
WHERE a.Usercode='{0}' 
	    and a.Pwd='{1}'
      and HasDeleted=0
    ]]>
