        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Title="Window011" Height="120" Width="160">
    <Grid Background="LightBlue">
        <ListBox Margin="5">
            <ListBox.ItemsSource>
                <x:Array Type="sys:String">
